{
  "name": "<%= name %>",
  "version": "0.1.0",
  "description": "Chaincodes package for <%= name %>",
  "files": [
    "dist/*"
  ]<% if(scripts) { %>,
  "scripts": { <% for(let [i, script] of scripts.entries()) { %>
    "<%- script.name %>": "<%- script.value %>"<% if((i + 1) !== (scripts.length)) { %>, <% } %><% } %>
  }<% } %><% if(dependencies) { %>,
  "dependencies": { <% for(let [i, dependency] of dependencies.entries()) { %>
    "<%- dependency.name %>": "<%- dependency.value %>"<% if((i + 1) !== (dependencies.length)) { %>, <% } %><% } %>
  }<% } %><% if(devDependencies) { %>,
  "devDependencies": {  <% for(let [i, devDependency] of devDependencies.entries()) { %>
    "<%- devDependency.name %>": "<%- devDependency.value %>"<% if((i + 1) !== (devDependencies.length)) { %>, <% } %> <% } %>
  } <% } %>
}

